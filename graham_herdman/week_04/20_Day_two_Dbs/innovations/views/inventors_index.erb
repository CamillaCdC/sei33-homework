
<div class="body_inventors_index">

  <div class="inventors_index_background">
    <div class="inventors_box">

      <h1 class="heading_box">INVENTORS</h1>

      <% @inventors.each do |inventor| %>
        <ul>
          <li class="inventor"><a href="/inventors/<%= inventor["id"] %>">
            <%= inventor["name"] %>
          </a></li>
        </ul>
      <% end %>

    </div>
  </div>

</div>
